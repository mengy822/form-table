{
  "name": "form-table-vue3",
  "version": "0.0.7",
  "private": false,
  "type": "module",
  "description": "Vue3中基于Element Plus二次封装基础表单库",
  "author": "bst",
  "license": "MIT",
  "keywords": [
    "element-plus",
    "vitepress",
    "vue3",
    "my-component",
    "二次封装组件",
    "封装组件"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "packages"
  ],
  "scripts": {
    "dev": "kill -9 $(netstat -nlp | grep :80 | awk '{print $7}' | cut -d'/' -f1) & vite",
    "dev-win": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build:lib": "vite build --mode lib",
    "build:docs": "cd docs && pnpm build",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "push": "npm config set registry=https://registry.npmjs.org & npm publish --access public & npm config set registry https://registry.npmmirror.com/"
  },
  "peerDependencies": {
    "element-plus": "^2.9.8",
    "vue": "^3.5.15"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^7.5.4",
    "terser": "^5.44.0",
    "vue": "^3.5.15",
    "@element-plus/icons-vue": "^2.3.1",
    "vue-tsc": "^2.1.10"
  }
}