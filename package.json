{
  "name": "form-table-vue3",
  "version": "0.0.5",
  "private": false,
  "main": "./dist/form-table-vue3.umd.js",
  "type": "module",
  "scripts": {
    "dev": "kill -9 $(netstat -nlp | grep :80 | awk '{print $7}' | cut -d'/' -f1) & vite",
    "dev-win": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "push": "npm config set registry=https://registry.npmjs.org & npm publish --access public & npm config set registry https://registry.npmmirror.com/"
  },
  "peerDependencies": {
    "element-plus": "2.9.1",
    "vue": "^3.4.31"
  },
  "dependencies": {
    "vue-types": "^5.0.3"
  },
  "devDependencies": {
    "unplugin-auto-import": "^0.18.6",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "typescript": "~5.6.3",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  }
}